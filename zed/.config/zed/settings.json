{
  "base_keymap": "VSCode",
  "vim_mode": true,
  "show_edit_predictions": false,
  "features": {
    "edit_prediction_provider": "zed"
  },
  "buffer_font_size": 15.0,
  "buffer_font_features": {
    // Disable ligatures:
    "calt": false
  },
  "cursor_blink": false,
  "format_on_save": "on",
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tabs": {
    "close_position": "left"
  },
  "preview_tabs": {
    "enabled": false
  },
  "terminal": {
    "shell": {
      "program": "zsh"
    }
  },
  "wrap_guides": [100],
  "languages": {
    "CSS": {
      "tab_size": 2,
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      }
    },
    "Dart": {
      "tab_size": 2
    },
    "HTML": {
      "tab_size": 2,
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      }
    },
    "JavaScript": {
      "tab_size": 2,
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      }
    },
    "Markdown": {
      "show_edit_predictions": false,
      "soft_wrap": "bounded"
    },
    "TSX": {
      "tab_size": 2,
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      }
    },
    "TypeScript": {
      "tab_size": 2,
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      }
    },
    "Rust": {
      "preferred_line_length": 100
    }
  },
  "theme": "Min Dark (Solid)"
